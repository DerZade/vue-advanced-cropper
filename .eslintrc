{
	"extends": [
		"plugin:vue/recommended"
	],
	"env": {
		"node": true
	},
	"parserOptions": {
		"parser": "babel-eslint"
	},
	"plugins": [
		"vue"
	],
	"rules": {
		"space-before-function-parent": 0,
		"quotes": [2, "single"],
		"indent": [
			2,
			"tab"
		],
		"no-tabs": "off",
		"vue/require-default-prop": "off"
	}
}
